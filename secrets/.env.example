###############################################
# Copy this file to secrets/.env and fill values
# Never commit secrets/.env to version control.
###############################################

# CoinGlass API
# Required for Phase 1 ingest. Keep this secret.
COINGLASS_API_KEY=

# Optional client-side rate limit (requests per minute)
# Helps avoid 429s when slicing windows in ingest.
CG_RPM_LIMIT=280

# Auto-deploy to Jetson (used by scripts/deploy_auto.sh)
# When all variables below are set, scripts/train_full.sh will
# package the ONNX + configs and push to the Jetson after P8.
AUTO_DEPLOY=1

# Jetson SSH connection
JETSON_HOST=192.168.1.50
JETSON_USER=deploy
# Absolute path to SSH private key used for deployment
JETSON_SSH_KEY=~/.ssh/jetson_deploy

# Jetson app root (where bundles will be unpacked)
# The unpack script maintains versioned dirs and swaps symlinks under this root:
#   /opt/app/models/current.onnx -> /opt/app/models/V_YYYYmmddHHMM/model.onnx
JETSON_REMOTE_ROOT=/opt/app

# End of file
